   COMMENT ON TABLE "TOAKDBI"."DBI_REQUEST_HISTORY"  IS 'History table allows tracing back the DBIÂ’s workflow events like approvals, rejections, test history etc. Populated by trigger DBI_REQUEST_TR when the status is changed.';
  GRANT SELECT ON "TOAKDBI"."DBI_REQUEST_HISTORY" TO "ADBM";
